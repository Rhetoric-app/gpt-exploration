\pard \li13\ri13\sb166 \brdrt\brdrs\brdrw13\brdrcf2\brdrr\brdrs\brdrw13\brdrcf2\brdrb\brdrs\brdrw13\brdrcf2\brdrl\brdrs\brdrw13\brdrcf2\sl-13\qc {\*\bkmkstart BKMK_186}{\*\bkmkend BKMK_186}\par
